{
  "name": "jeremy-laurent/la-guilde-des-seigneurs",
  "type": "project",
  "description": "API La Guilde des Seigneurs",
  "license": "proprietary",
  "authors": [
    {
      "name": "Jérémy Laurent",
      "email": "jeremy.laurent@etu.univ-smb.fr",
      "homepage": "https://github.com/PoulpY2K",
      "role": "Developer"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.0.2",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/annotations": "^1.13",
    "doctrine/doctrine-bundle": "^2.5",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^2.11",
    "nelmio/api-doc-bundle": "^4.8",
    "phpdocumentor/reflection-docblock": "^5.3",
    "phpstan/phpdoc-parser": "^1.2",
    "sensio/framework-extra-bundle": "^6.2",
    "symfony/asset": "6.0.*",
    "symfony/console": "^6",
    "symfony/dotenv": "^6",
    "symfony/event-dispatcher": "^6",
    "symfony/expression-language": "^6",
    "symfony/finder": "^6",
    "symfony/flex": "^2",
    "symfony/form": "^6",
    "symfony/framework-bundle": "^6",
    "symfony/monolog-bundle": "^3.7",
    "symfony/property-access": "^6",
    "symfony/property-info": "^6",
    "symfony/proxy-manager-bridge": "^6",
    "symfony/runtime": "^6",
    "symfony/security-bundle": "^6",
    "symfony/serializer": "^6",
    "symfony/twig-bundle": "^6",
    "symfony/validator": "^6",
    "symfony/yaml": "^6",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "phpstan/phpstan": "^1.4",
    "phpstan/phpstan-symfony": "^1.1",
    "phpunit/phpunit": "^9.5",
    "rector/rector": "^0.12.16",
    "symfony/browser-kit": "^6",
    "symfony/css-selector": "^6",
    "symfony/maker-bundle": "^1.36",
    "symfony/phpunit-bridge": "^6",
    "symfony/stopwatch": "^6",
    "symfony/web-profiler-bundle": "^6"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "^6",
      "docker": false
    }
  }
}
